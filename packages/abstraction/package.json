{
  "name": "@penguin-tech/abstraction",
  "version": "1.0.1",
  "description": "Penguin Techology Abstraction",
  "author": "huynvq",
  "license": "ISC",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "nest build"
  },
  "dependencies": {
    "@penguin-tech/core": "^1.0.1",
    "@penguin-tech/crawler": "^1.0.1",
    "@penguin-tech/mongo": "^1.0.1",
    "@penguin-tech/proxy-manager": "^1.0.1",
    "@penguin-tech/rabbit": "^1.0.1",
    "@penguin-tech/redis": "^1.0.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
